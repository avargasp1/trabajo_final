<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CelularServ</servlet-name>
        <servlet-class>CelularServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Api</servlet-name>
        <servlet-class>Api</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LenguajeServ</servlet-name>
        <servlet-class>LenguajeServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SistemaServ</servlet-name>
        <servlet-class>SistemaServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModeloServ</servlet-name>
        <servlet-class>ModeloServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarcaServ</servlet-name>
        <servlet-class>MarcaServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CelularServ</servlet-name>
        <url-pattern>/CelularServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Api</servlet-name>
        <url-pattern>/Api</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LenguajeServ</servlet-name>
        <url-pattern>/LenguajeServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SistemaServ</servlet-name>
        <url-pattern>/SistemaServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModeloServ</servlet-name>
        <url-pattern>/ModeloServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarcaServ</servlet-name>
        <url-pattern>/MarcaServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
